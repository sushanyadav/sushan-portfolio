{
  "name": "sushan-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "next-lint": "next lint",
    "lint-fix": "eslint --cache --fix .",
    "prettier-format": "prettier --write .",
    "check-prettier-format": "prettier --check .",
    "check-lint": "eslint .",
    "test-all-and-build": "npm run next-lint && npm run check-lint && npm run check-prettier-format && npm run build",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx}": "npm run lint-fix",
    "*.{js,jsx,css,scss,md}": "npm run prettier-format"
  },
  "dependencies": {
    "gsap": "^3.7.0",
    "locomotive-scroll": "^4.1.1",
    "next": "11.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.35.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "eslint": "^7.29.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "^11.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-tailwindcss": "^1.13.4",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.2.13",
    "prettier": "^2.3.1",
    "tailwindcss": "^2.2.2"
  }
}
