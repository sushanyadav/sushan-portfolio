{
  "name": "sushan-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prettier-quick": "pretty-quick --staged",
    "check-format": "prettier --check src/**/*.{js,jsx} src/pages/**/*.{js,jsx}",
    "format": "prettier --write src/**/*.{js,jsx} src/pages/**/*.{js,jsx}",
    "lint-fix": "eslint --fix src/**/*.{js,jsx} src/pages/**/*.{js,jsx}",
    "check-lint": "eslint --ext js --ext jsx src/**/*.{js,jsx} src/pages/**/*.{js,jsx}",
    "test-all-and-build": "npm run lint-fix && npm run check-format && npm run check-lint && npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "gsap": "^3.7.0",
    "locomotive-scroll": "^4.1.1",
    "next": "11.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.35.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "eslint": "^7.29.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "^11.0.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "postcss": "^8.2.13",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.1",
    "tailwindcss": "^2.2.2"
  }
}
